{
	"name": "dolly",
	"publisher": "xieyuzhong",
	"displayName": "Dolly - Generate Folder of Files from Template",
	"description": "Generate folder of files from template in Visual Studio Code",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dolly"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.dolly",
				"title": "Dolly Go!"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.dolly",
					"title": "Dolly Go!",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "Dolly Configuration",
			"properties": {
				"dolly.templatePath": {
					"type": "string",
					"default": "./node_modules/vscode-docs/react/componentTemplate/Robot",
					"description": "Where the template is stored relative to the workspace root path. Defaults to a folder named './node_modules/vscode-docs/react/componentTemplate/Robot' in the root of your project."
				},
				"dolly.newFolderName": {
					"type": "string",
					"default": "children",
					"description": "Name of the generated folder."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/fs-extra": "^8.0.0",
		"fs-extra": "^8.1.0"
	}
}
